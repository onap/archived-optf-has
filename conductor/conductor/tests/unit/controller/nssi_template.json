{
   "name":"urllc_sample",
   "files":{

   },
   "limit":10,
   "num_solution":"10",
   "timeout":1200,
   "template":{
      "homing_template_version":"2018-02-01",
      "demands":{
         "URLLC_core":[
            {
               "inventory_provider":"aai",
               "inventory_type":"nssi",
               "unique":"true",
               "region":"RegionOne",
               "filtering_attributes":{
                  "service-role":"nssi",
                  "orchestration-status":"active",
                  "model-invariant-id":"21d57d4b-52ad-4d3c-a798-248b5bb9124a",
                  "model-version-id":"bfba363e-e39c-4bd9-a9d5-1371c28f4d22"
               }
            }
         ],
         "URLLC_ran":[
            {
               "inventory_provider":"aai",
               "inventory_type":"nssi",
               "unique":"true",
               "region":"RegionOne",
               "filtering_attributes":{
                  "service-role":"nssi",
                  "orchestration-status":"active",
                  "model-invariant-id":"aa2d56ea-773d-11ea-bc55-0242ac130003",
                  "model-version-id":"d6296806-773d-11ea-bc55-0242ac130003"
               }
            }
         ]
      },
      "constraints":{
         "URLLC_core_Threshold":{
            "type":"threshold",
            "demands":[
               "URLLC_core"
            ],
            "properties":{
               "evaluate":[
                  {
                     "attribute":"latency",
                     "operator":"lte",
                     "threshold":30,
                     "unit":"ms"
                  },
                  {
                     "attribute":"reliability",
                     "operator":"gte",
                     "threshold":99.99,
                     "unit":""
                  }
               ]
            }
         },
         "URLLC_ran_Threshold":{
            "type":"threshold",
            "demands":[
               "URLLC_ran"
            ],
            "properties":{
               "evaluate":[
                  {
                     "attribute":"latency",
                     "operator":"lte",
                     "threshold":30,
                     "unit":"ms"
                  }
               ]
            }
         },
         "URLLC_aggregation":{
            "type":"aggregation",
            "demands":[
               "URLLC_core",
               "URLLC_ran"
            ],
            "properties":{
               "evaluate":[
                  {
                     "attribute":"latency",
                     "function":"sum",
                     "operator":"lte",
                     "threshold":70,
                     "unit":""
                  }
               ]
            }
         }
      },
      "optimization":{
         "minimize":{
            "sum":[
               {
                  "latency":"URLLC_core"
               },
               {
                  "latency":"URLLC_ran"
               }
            ]
         }
      }
   }
}
